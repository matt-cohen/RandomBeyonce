<form method="post" action= "/sets" id="editor">
	Edit a setlist: 
	<br />
	Set Name: <input type="text" name="name" size="50" value= "<%= set["name"]  %>"> 
	<br />
	

	Videos: 

	<textarea rows = "10" cols = "50" name = "vidnums" form = "editor">
<% set["vidnums"].each do |number| %>
<%= number %>

<% end %></textarea>

	<br />
	



	Description: <input type="text" name="description" size="120" value="<%= set['description'] %>"/>
	<br />
	<input type="submit" value="Edit your set!"/>
</form>



